      <access>read-write</access>

  <interrupt><name>GPIO_0_IRQ</name><value>8</value></interrupt>
      <interrupt><name>GPIO_1_IRQ</name><value>9</value></interrupt>
      <interrupt><name>GPIO_2_IRQ</name><value>10</value></interrupt>
